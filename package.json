  "scripts": {
    "test": "npm run test:api:unit && npm run test:api:integration && npm run test:client:unit && npm run test:client:integration",
    "test:client:integration": "node --harmony node_modules/karma/bin/karma start --single-run",
    "test:client:integration:dev": "node --harmony node_modules/karma/bin/karma start",
    "test:client:unit": "node --harmony node_modules/mocha-webpack/bin/mocha-webpack --webpack-config webpack.config-mocha.js \"public/test/unit/**/*.unit.js\"",
    "test:api:unit": "node --harmony ./node_modules/mocha/bin/mocha --require ./test/requirements/mocha.js --colors ./test/integration/**/*.int.js",
    "test:api:integration": "node --harmony ./node_modules/mocha/bin/mocha --require ./test/requirements/mocha.js --colors ./test/unit/**/*.unit.js",
    "build:test:client": "node node_modules/webpack/bin/webpack.js --config webpack.config-mocha.js",
    "build:test:client:unit": "npm run build:test:client",
    "build:test:client:integration": "npm run build:test:client",
    "build:client": "node node_modules/webpack/bin/webpack.js --config webpack.config.js",
    "serve:test:client:integration:webpack": "echo \"http://localhost:8010/webpack-dev-server/\" && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config-mocha.js --port 8010 --content-base \"./dist/test/integration-client\"",
    "serve:client": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.js --content-base \"./dist/\"",
    "serve:api": "npm run start",
    "start:dev": "node --harmony ./src/app.js",
    "start": "NODE_ENV=production node_modules/forever/bin/forever start -c \"node --harmony\" ./src/app.js ",
    "stop": "node_modules/forever/bin/forever stop ./src/app.js",
    "start:win32": "set NODE_ENV=production && node node_modules/forever/bin/forever start -c \"node --harmony\" ./src/app.js ",
    "stop:win32": "node node_modules/forever/bin/forever stop ./src/app.js"
  },
