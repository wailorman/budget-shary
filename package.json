{
  "name": "budget-shary",
  "version": "2.3.0-beta",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run-script build && npm run-script test:unit",
    "test:integration": "node --harmony node_modules/karma/bin/karma start --single-run",
    "test:integration:dev": "node --harmony node_modules/karma/bin/karma start",
    "test:unit:old": "node --harmony node_modules/mocha-webpack/bin/mocha-webpack --webpack-config webpack.config.js \"**/*.unit.js\"",
    "test:unit": "node ./node_modules/mocha/bin/mocha --opts mocha.opts **/*.unit.js",
    "build": "node node_modules/webpack/bin/webpack.js --progress --config webpack.config.js",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.js --progress --content-base \"./\"",
    "deploy": "gulp deploy",
    "predeploy:win32": "rm -rf ./dist/js && set NODE_ENV=production& npm run build && gulp zip",
    "lint": "./node_modules/.bin/eslint ./src",
    "cover": "node ./node_modules/cross-env/bin/cross-env.js ./node_modules/nyc/bin/nyc.js npm run test:unit",
    "storybook": "./node_modules/.bin/start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wailorman/budget-shary.git"
  },
  "author": "Sergey Popov <wailorman@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wailorman/budget-shary/issues"
  },
  "homepage": "https://github.com/wailorman/budget-shary",
  "babel": {
    "retainLines": true,
    "presets": [
      "es2015",
      "react",
      "stage-1"
    ],
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread",
      "transform-decorators-legacy",
      "istanbul",
      [
        "transform-es2015-modules-commonjs-simple",
        {
          "noMangle": true
        }
      ]
    ]
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "function": 100,
    "lines": 100,
    "statements": 100,
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "sourceMap": false,
    "instrument": false,
    "require": [
      "babel-register"
    ]
  },
  "dependencies": {
    "async": "^1.5.2",
    "babel-core": "^6.22.1",
    "babel-istanbul": "^0.12.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.13",
    "chai-immutable": "^1.6.0",
    "css-loader": "^0.24.0",
    "dexie": "^1.2.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.0",
    "gulp-zip": "^3.2.0",
    "immutable": "^3.8.1",
    "imports-loader": "^0.6.5",
    "jquery": "^2.1.4",
    "json-loader": "^0.5.4",
    "karma-coverage": "^1.1.1",
    "localStorage": "^1.0.3",
    "lodash": "^4.17.4",
    "material-ui": "^0.16.4",
    "null-loader": "^0.1.1",
    "nyc": "^10.1.2",
    "q": "^1.4.1",
    "raw-loader": "^0.5.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "^2.0.1",
    "recompose": "^0.22.0",
    "redux": "^3.3.1",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.0.1",
    "reselect": "^2.5.4",
    "shortid": "^2.2.6",
    "store": "^1.3.20",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.7",
    "validate.js": "^0.10.0",
    "webpack": "^1.12.9",
    "webpack-node-externals": "^1.0.0",
    "write-file-webpack-plugin": "^3.1.8"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "atom-ui-reporter": "^0.0.1",
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-source-map-support-for-6": "^0.0.5",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babel-polyfill": "^6.20.0",
    "chai": "^3.4.1",
    "chai-enzyme": "^0.5.1",
    "cross-env": "^3.1.4",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.5.1",
    "eslint": "^3.8.1",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.4.1",
    "gulp-util": "^3.0.7",
    "isparta": "^4.0.0",
    "istanbul": "1.0.0-alpha.2",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-live-preprocessor": "^0.2.2",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.2.0",
    "mocha-loader": "^1.1.0",
    "mocha-testdata": "^1.2.0",
